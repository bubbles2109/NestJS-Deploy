{
    "version": 2,
    "builds": [
      {
        "src": "path/to/your/source",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/main.js",
        "methods": [
            "GET",
            "POST",
            "PUT",
            "DELETE"
        ]
      }
    ]
  }